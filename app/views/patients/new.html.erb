<%= form_for :patient, url: patients_url, method: :post do |f| %>
  <input type="hidden" name="verification_token" value="<%= params[:verification_token] %>">
  <label>Name:</label>
  <input type="text" name="patient[name]" value="<%= @patient.name %>">
  <br>
  <label>Email: </label>
  <input type="text" name="patient[email]" value="<%= @patient.email %>">
  <br>
  <label>Password: </label>
  <input type="password" name="patient[password]" value="<%= @patient.password %>">
  <br>
  <label >Phone Number:</label>
  <input type="text" name="patient[phone_number]" value="<%= @patient.phone_number %>">
  <br>
  <label >Insurance Company:</label>
  <input type="text" name="patient[insurance_company]" value="<%= @patient.insurance_company %>">
  <br>
  <input type="submit" value="Create Account!">
<% end %>
